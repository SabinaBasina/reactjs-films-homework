{
  "name": "reactjs-films-homework",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --config webpack/webpack.prod.js",
    "start": "SET NODE_ENV=development&&node server.js ",
    "prod": "webpack --config webpack/webpack.prod.js && node server.js",
    "lint": "eslint --debug src/ && stylelint src/**/*.scss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "./src/": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SabinaBasina/reactjs-films-homework.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SabinaBasina/reactjs-films-homework/issues"
  },
  "homepage": "https://github.com/SabinaBasina/reactjs-films-homework#readme",
  "dependencies": {
    "@okta/okta-react": "^1.0.3",
    "@okta/okta-signin-widget": "^2.17.0",
